		duck du = new duck();
	    du.duckfly();
	    // 출력 : 오리가 날개로 날아갑니다.
	    
	    cjdendduck a청둥오리 = new cjdendduck();
	    a청둥오리.fly();
	    a청둥오리.duckswim();
	    // 출력 : 오리가 날개로 날아갑니다.
	    
	    gmlsduck a흰오리 = new gmlsduck();
	    a흰오리.fly();
	    a흰오리.duckswim();
	    // 출력 : 오리가 날개로 날아갑니다.
	    
	    rhanduck a고무오리 = new rhanduck();
	    a고무오리.fly();
	    a고무오리.swim();
	    
