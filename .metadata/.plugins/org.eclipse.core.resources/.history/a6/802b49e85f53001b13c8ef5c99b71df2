package exm1;

public class duck {
	
	
	
	
	
	
	void duckfly() {
		System.out.println("오리가 날개로 날아갑니다.");
	}

}

class cjdendduck extends duck{
	void fly() {
		duckfly();
	}
}
class gmlsduck extends duck{
	void fly() {
		duckfly();
	}
}
class rhanduck extends duck{
	void fly() {
		System.out.println("저는 날 수 없어요. ㅜㅠ");
	}
}
class rhanduck2 extends duck{
	
}
